    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.89.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
